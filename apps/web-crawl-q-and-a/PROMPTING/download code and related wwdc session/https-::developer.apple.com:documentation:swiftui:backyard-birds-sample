<span data-v-6bfdd231="" data-v-16d388ce="" class="code-line-container"><!----><span data-v-6bfdd231="" data-v-16d388ce="" class="code-line">    </span></span>
<span data-v-6bfdd231="" data-v-16d388ce="" class="code-line-container"><!----><span data-v-6bfdd231="" data-v-16d388ce="" class="code-line">    <span class="syntax-keyword">public</span> <span class="syntax-keyword">var</span> info: <span class="syntax-type">BirdSpeciesInfo</span> { <span class="syntax-type">BirdSpeciesInfo</span>(rawValue: id) }</span></span>
<span data-v-6bfdd231="" data-v-16d388ce="" class="code-line-container"><!----><span data-v-6bfdd231="" data-v-16d388ce="" class="code-line">    </span></span>
<span data-v-6bfdd231="" data-v-16d388ce="" class="code-line-container"><!----><span data-v-6bfdd231="" data-v-16d388ce="" class="code-line">    <span class="syntax-keyword">public</span> <span class="syntax-keyword">init</span>(<span class="syntax-params">info</span>: <span class="syntax-type">BirdSpeciesInfo</span>, <span class="syntax-params">naturalScale</span>: <span class="syntax-type">Double</span> <span class="syntax-operator">=</span> <span class="syntax-number">1</span>, <span class="syntax-params">isEarlyAccess</span>: <span class="syntax-type">Bool</span> <span class="syntax-operator">=</span> <span class="syntax-literal">false</span>, <span class="syntax-params">parts</span>: [<span class="syntax-type">BirdPart</span>]) {</span></span>
